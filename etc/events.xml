<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../lib/internal/Magento/Framework/Event/etc/events.xsd">
    <event name="tokenbase_before_load_active_cards">
        <observer name="tokenbase_check_queue_deletion" instance="ParadoxLabs\TokenBase\Model\Observer\CardLoad" method="checkQueuedForDeletion" />
    </event>
    <event name="sales_order_payment_capture">
        <observer name="tokenbase_capture" instance="ParadoxLabs\TokenBase\Model\Observer\Capture" method="processCapture" />
    </event>
    <event name="sales_order_creditmemo_refund">
        <observer name="tokenbase_refund" instance="ParadoxLabs\TokenBase\Model\Observer\Refund" method="processRefund" />
    </event>
    <event name="sales_model_service_quote_submit_before">
        <observer name="tokenbase_convert_quote_to_order" instance="ParadoxLabs\TokenBase\Model\Observer\Convert" method="quoteToOrder" />
    </event>
    <event name="checkout_type_multishipping_create_orders_single">
        <observer name="tokenbase_convert_quote_to_order" instance="ParadoxLabs\TokenBase\Model\Observer\Convert" method="quoteToOrder" />
    </event>
    <event name="sales_convert_order_to_quote">
        <observer name="tokenbase_convert_order_to_quote" instance="ParadoxLabs\TokenBase\Model\Observer\Convert" method="orderToQuote" />
    </event>
</config>
